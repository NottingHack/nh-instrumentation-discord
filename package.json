{
  "name": "nh-discord",
  "version": "0.1.0",
  "description": "A discord bot for nottingham hackspace",
  "main": "main.js",
  "scripts": {
    "test": "node main.js",
    "dev:run-mqtt": "docker run -it -p 1883:1883 eclipse-mosquitto:latest mosquitto -c /mosquitto-no-auth.conf",
    "docker-build": "docker build -t nh-discord .",
    "docker-run": "docker run -v ./config.json:/nh-discord/config.json -it nh-discord:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AaronJackson/nh-discord.git"
  },
