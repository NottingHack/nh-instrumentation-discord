  "bugs": {
    "url": "https://github.com/AaronJackson/nh-discord/issues"
  },
  "homepage": "https://github.com/AaronJackson/nh-discord#readme",
  "dependencies": {
    "canvas": "^3.2.0",
    "chart.js": "^4.5.0",
    "discord.js": "^14.24.2",
    "express": "^4.21.2",
    "jsdom": "^27.0.1",
    "mqtt": "^5.10.3",
    "prom-client": "^15.1.3",
    "qr-code-styling": "^1.9.2",
    "skia-canvas": "^2.0.2",
    "wtf_wikipedia": "^10.4.0",
    "wtf-plugin-markdown": "^1.0.1"
  }
